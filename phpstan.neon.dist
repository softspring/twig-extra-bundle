includes:
	- phpstan-baseline.neon

parameters:
    level: 5
    paths:
        - src/
        - tests/
    bootstrapFiles:
        - vendor/autoload.php
    ignoreErrors:
        - '#Call to an undefined method Symfony\\Component\\Config\\Definition\\Builder\\NodeDefinition::addDefaultsIfNotSet\(\).#'
        - '#Used function twig_date_format_filter not found.#'
        - '#Function twig_date_format_filter not found.#'